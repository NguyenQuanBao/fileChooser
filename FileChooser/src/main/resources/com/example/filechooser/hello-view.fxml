<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" fx:id="vb"
      fx:controller="com.example.filechooser.HelloController">
    <children>
        <ImageView fx:id="imageView" fitHeight="224.0" fitWidth="396.0"
                   layoutX="100.0" layoutY="50.0" pickOnBounds="true" preserveRatio="true"/>

        <Button layoutX="256.0" layoutY="300.0" mnemonicParsing="false" onAction="#chooseImage" text="Open"/>
    </children>
</VBox>
